<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="document">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="section" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
              <xs:element name="header">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="b">
                      <xs:complexType mixed="true">
                        <xs:sequence>
                          <xs:element name="span" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:string" name="class"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="p">
                <xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element type="xs:string" name="i" minOccurs="0"/>
                    <xs:element type="xs:string" name="sup" minOccurs="0"/>
                    <xs:element name="b" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="i"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element type="xs:string" name="sub" minOccurs="0"/>
                    <xs:element name="video" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="source">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:string" name="src"/>
                                  <xs:attribute type="xs:string" name="type"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="chapter" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="xs:time" name="timestamp" use="optional"/>
                                  <xs:attribute type="xs:string" name="label" use="optional"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute type="xs:short" name="width"/>
                        <xs:attribute type="xs:byte" name="height"/>
                        <xs:attribute type="xs:string" name="controls"/>
                        <xs:attribute type="xs:string" name="title"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="div">
                <xs:complexType mixed="true">
                  <xs:sequence>
                    <xs:element type="xs:string" name="p" minOccurs="0"/>
                    <xs:element name="div" minOccurs="0">
